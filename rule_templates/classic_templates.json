{
  "koch_snowflake": {
    "base_axiom": ["F", "-", "-", "F", "-", "-", "F"],
    "rules": [
      {
        "pattern": "F",
        "replacement": "F+F--F+F",
        "probability": 1.0,
        "axiom": false
      }
    ],
    "params": {
      "length": 10,
      "width": 3,
      "angle": 60,
      "iterations": 8
    },
    "drawer_name": "geometric"
  },
